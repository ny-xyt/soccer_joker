# State 1
STARTSTATE
reset LEG_LEFT LEG_RIGHT end
inctar EFF_LL3 19 EFF_LL4 -40 EFF_LL5 20 EFF_RL2 40 EFF_RL3 19 EFF_RL4 -50 EFF_RL5 20 end
wait 2 end
ENDSTATE



ENDSKILL 

